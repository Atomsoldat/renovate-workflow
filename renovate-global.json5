{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "branchPrefix": "github-renovate/",
  "dependencyDashboardTitle": "Dependency Dashboard self-hosted",
  "gitAuthor": "Leon Welchert <lichturm@posteo.de>",
  "onboarding": true,
  "onboardingBranch": "github-renovate/configure",
  "platform": "github",
  "autodiscover": "true",
  // text enclosed in slashes gets treated as regex
  "autodiscoverFilter":  ["/MausoleumManagement/.*/", "/Atomsoldat/lichturm-website/"]
  "packageRules": [
    { "matchCategories": ["docker"],
      "pinDigests": true
    }
  ]
}
