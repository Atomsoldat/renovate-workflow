{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
 
  // https://docs.renovatebot.com/config-presets/
  // https://docs.renovatebot.com/presets-config/
  "extends": [ "config:best-practices" ],

  "branchPrefix": "github-renovate/",
  "dependencyDashboardTitle": "Dependency Dashboard self-hosted",
  "assignees": ["Atomsoldat"],
  // take a random sample from the list of maintainers
  "assigneesSampleSize": 1,
  "onboarding": true,
  "onboardingBranch": "github-renovate/configure",
  "platform": "github",
  "autodiscover": true,
  "autodiscoverFilter": [
    "/MausoleumManagement/.*/",
    "/Atomsoldat/lichturm-website/"
  ],
  "packageRules": [{"matchCategories": ["docker"], "pinDigests": true}]
}

